<!DOCTYPE html>
<!--[if IE 9 ]><html class="ie9 no-js"><![endif]-->
<!--[if IE 8 ]><html class="ie8 no-js"><![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--><html class="no-js"><!--<![endif]-->
<head>
  <meta charset="utf-8" />

  <!-- <link rel="icon" type="image/png" href="{{ 'favicon.png' | asset_url }}"/> -->
  <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1' /><![endif]-->
  <!--[if lt IE 9]>{{ '//html5shiv.googlecode.com/svn/trunk/html5.js' | script_tag }}<![endif]-->

  <link rel="icon" type="image/png" href="{{ 'favicon.png' | asset_url }}"/>

  <link rel="canonical" href="{{ canonical_url }}" />
  <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no" />

  {% include 'u-social-meta' %}

  <noscript>{{ 'style.scss.css' | asset_url | stylesheet_tag}}</noscript>
  
  {{ 'modernizr.min.js' | asset_url | script_tag}}
  
  {% if settings.in_development == '1' %}
    {{ 'style.scss.css' | asset_url | stylesheet_tag}}
  {% else %}
    {% include 'u-critical' %}
  {% endif %}

  {{ content_for_header }}

  <title>
    {{ page_title }}{% if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif %}{% if current_page != 1 %} &ndash; Page {{ current_page }}{% endif %}{% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}
  </title>
  
  <!--Start of Zopim Live Chat Script-->
  <script type="text/javascript">
  var ua = navigator.userAgent.toLowerCase(),
  platform = navigator.platform.toLowerCase();
  platformName = ua.match(/ip(?:ad|od|hone)/) ? 'ios' : (ua.match(/(?:webos|android)/) || platform.match(/mac|win|linux/)
   || ['other'])[0],
  isMobile = /ios|android|webos/.test(platformName);
  
  if (!isMobile) {
  window.$zopim||(function(d,s){var z=$zopim=function(c){z._.push(c)},$=z.s=
  d.createElement(s),e=d.getElementsByTagName(s)[0];z.set=function(o){z.set.
  _.push(o)};z._=[];z.set._=[];$.async=!0;$.setAttribute('charset','utf-8');
  $.src='//v2.zopim.com/?2ls5afy8PQp3skrpUgBoLsGzhXwVI5Ca';z.t=+new Date;$.
  type='text/javascript';e.parentNode.insertBefore($,e)})(document,'script');
  }
  </script>
    
    <script>
   
    $zopim(function() {
      $zopim.livechat.concierge.setName('Gitman');
      $zopim.livechat.window.setTitle('Gitman Live Chat');
      $zopim.livechat.concierge.setAvatar('{{"logo.png" | asset_url }}');
      $zopim.livechat.theme.setColor('#b99d6b');
      $zopim.livechat.theme.reload(); // apply new theme settings
    });
  
   
  </script>
    
  <!--End of Zopim Live Chat Script-->
</head>

{% capture body_class %}
  {% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '.', ' ' | handle }}
{% endcapture %}
{% capture root_template %}{{ template }}{% endcapture %}

<body id="{{ page_title | handle }}" class="{{ body_class | strip }} critical-loading" data-dev="{{settings.in_development}}">
  <span id="js-progress-bar" class="js-critical-progress-bar"></span>
   
   <!-- Google Tag Manager -->
  <noscript>
    <iframe src="//www.googletagmanager.com/ns.html?id=GTM-KD2XGW" height="0" width="0" style="display:none;visibility:hidden"></iframe>
  </noscript>
  
  <script>(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
  new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
  j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
  '//www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
  })(window,document,'script','dataLayer','GTM-KD2XGW');</script>
  <!-- End Google Tag Manager -->
  
   <div class="critical">
    <div class="o-loader">
      <i></i>
      <i></i>
      <i></i>
      <i></i>
      <i></i>
    </div> 
  </div>
  
  {% include 'site-header' %}

  <div id="ajax-content" class="page-wrap h-s1 block-relative non-critical mobile-canvas js-canvas">
    {{ content_for_layout }}

    {% include 'site-footer' %}
    {% include 'modal-styleandfit' %}
  </div> <!-- END .page-wrap -->
  
  {% include 'site-header--mobile' %}
  
  <div class="hide js-cta-storefinder mt--15">
    <div class="storefinder__inner row">
      {% include 'module-cta--newsletter' %}
    </div>
  </div>

  <style>
    @font-face {
      font-family: 'gitman-icons';
      src: url({{'gitmanicons.eot' | asset_url}});
      src: url({{'gitmanicons.eot?#iefix' | asset_url}}) format('embedded-opentype'),
           url({{'gitmanicons.woff' | asset_url}}) format('woff'),
           url({{'gitmanicons.ttf' | asset_url}}) format('truetype'),
           url({{'gitmanicons.svg?#fontello' | asset_url}}) format('svg');
      font-weight: normal;
      font-style: normal;
    }
  </style>

  <script type="text/javascript">
    var ns = '__GT__';

    window[ns] = {};

    window[ns].devMode = false;
    {% if settings.in_development == '1' %}
    window[ns].devMode = true;
    {% endif %}

    var WebFontConfig = {
     typekit:{
        id: 'dya5qqj'
      },
      active:function(){
        window[ns].fontsActive = true;
      },
      timeout: 40000
    };
    
    var cdn = "{{'' | asset_url}}"
    ,   stripped = cdn.match(/^(.[^?]*)\//);
    
    if (typeof stripped[1] != 'undefined')
      cdn = stripped[1];
    
    window[ns].loaded = {};
    
    window[ns].assets = {
      cdn:cdn,
      css:{
        theme:"{{ 'style.scss.css' | asset_url }}"
      },
      js:{
        fonts:'{{ "//ajax.googleapis.com/ajax/libs/webfont/1/webfont.js" }}',
        vendor:'{{ "vendor.js" | asset_url }}',
        api:'{{ "api.jquery.js" | shopify_asset_url}}',
        countries:'/services/javascripts/countries.js',
        theme:'{{ "app.js" | asset_url }}'
      }
    };

    window[ns].json = {
      "product-tags":'{{ "product-tags.js" | asset_url }}',
      "mto-options-shirts":'{{ "mto-options-shirts.js" | asset_url }}',
      "mto-options-ties":'{{ "mto-options-ties.js" | asset_url }}',
      "mto-options-pants":'{{ "mto-options-pants.js" | asset_url }}'
    }
  
  window[ns].shipping = {
    "-1":"{{settings.ship_notification_label_deny}}",
    "0" :"{{settings.ship_notification_label_partial}}",
    "1" :"{{settings.ship_notification_label_none}}",
    "shipping":"{{settings.ship_notification_time}}"
  }
  </script>

  <div class="non-critical">
    {% include 'modal-register' %}
    {% include 'modal-login' %}
    {% include 'modal-monogram' %}
    {% include 'modal-quickview' %}
    {% include 'popover-added-to-cart' %}
  </div>

  {% if settings.in_development == '1' %}
    {{ "//ajax.googleapis.com/ajax/libs/webfont/1/webfont.js" | script_tag}}
    {{ "https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js" | script_tag}}
    {{ '/services/javascripts/countries.js' | script_tag }}
    {{ "vendor.min.js" | asset_url | script_tag}}
    {{ "app.js" | asset_url | script_tag}}
  {% else %}
    {{ "https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js" | script_tag}}
    {{ 'preload.js' | asset_url | script_tag }}
  {% endif %}

  {% include 'u-tracking-codes' %}
</body>
</html>
